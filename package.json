  "scripts": {
    "start": "nx serve demo",
    "start:server": "nx serve server",
    "start:documentation": "nx serve documentation --configuration=serve --port=4201",
    "build": "nx affected:build --parallel=3",
    "lint": "nx affected:lint --parallel=3",
    "lint:fix": "nx affected:lint --parallel=3 --fix",
    "format": "nx format:write",
    "format:check": "nx format:check",
