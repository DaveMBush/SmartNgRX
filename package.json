  "scripts": {
    "start": "nx serve demo",
    "start:prod": "nx serve demo --configuration=production",
    "start:server": "nx serve server",
    "start:documentation": "nx serve documentation --configuration=serve --port=4201",
    "build": "nx affected:build --parallel=3",
    "lint": "nx affected:lint --parallel=3",
    "lint:fix": "nx affected:lint --parallel=3 --fix",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "test:coverage": "nx affected:test --parallel=3 --max-parallel=3 --coverage --skip-nx-cache",
    "test": "nx affected:test --parallel=3 --max-parallel=3",
    "dupcheck": "pnpm jscpd",
    "htmllint": "pnpm htmlhint \"apps/**/*.html\" \"libs/**/*.html\" --ignore=\"coverage/**/*\"",
    "prepare": "is-ci || husky install",
    "postinstall": "pnpm dlx prisma migrate dev",
    "memlab": "tsx ./apps/demo-memlab/src/memlab/main.ts"
  },
  "prisma": {
    "seed": "pnpm ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,ts,css,scss,md,json,html}": [
      "pnpm format"
    ],
    "*.{js,ts,html}": [
      "pnpm lint"
    ],
    "*.html": [
      "pnpm htmllint"
    ]
  },
