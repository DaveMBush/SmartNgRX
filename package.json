    "dupcheck": "pnpm jscpd",
    "htmllint": "pnpm htmlhint \"apps/**/*.html\" \"libs/**/*.html\"",
    "prepare": "is-ci || husky install",
    "postinstall": "pnpm dlx prisma migrate dev",
    "memlab": "nx run demo-memlab:memlab"
  },
  "prisma": {
    "seed": "pnpm ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,ts,css,scss,md,json,html}": [
      "pnpm format"
    ],
    "*.{js,ts,html}": [
      "pnpm lint"
    ],
    "*.html": [
      "pnpm htmllint"
    ]
